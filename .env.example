# Supabase Configuration (Required for production)
REACT_APP_SUPABASE_URL=https://your-project.supabase.co
REACT_APP_SUPABASE_ANON_KEY=your-anon-key

# Google Gemini AI Configuration (Optional - for AI features)
REACT_APP_GEMINI_API_KEY=your-gemini-api-key

# Development Configuration
PORT=3001
GENERATE_SOURCEMAP=false

# PWA Configuration (Optional)
REACT_APP_PWA_NAME="Vigilantes del Aedes"
REACT_APP_PWA_SHORT_NAME="Vigilantes"

# Environment
REACT_APP_ENVIRONMENT=development

# Map Configuration (Optional)
REACT_APP_DEFAULT_LAT=-27.3671
REACT_APP_DEFAULT_LNG=-55.8961
REACT_APP_DEFAULT_ZOOM=13

# Features Flags (Optional)
REACT_APP_ENABLE_OFFLINE_MODE=true
REACT_APP_ENABLE_NOTIFICATIONS=true